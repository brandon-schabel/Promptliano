{
  "name": "promptliano-core",
  "module": "index.ts",
  "version": "0.12.0",
  "type": "module",
  "scripts": {
    "dev": "bun run scripts/start-dev.ts",
    "dev:client": "bun run scripts/start-client-dev.ts",
    "dev:server": "bun run scripts/start-server-dev.ts",
    "mcp:inspector": "MCP_AUTO_OPEN_ENABLED=false bun x @modelcontextprotocol/inspector",
    "build": "bun run scripts/build-all.ts",
    "build:binaries": "bun run scripts/build-binaries.ts",
    "build:vscode": "bun run --cwd packages/vscode-extension build",
    "test": "bun run --filter '*' test",
    "typecheck": "bun run --filter '*' typecheck",
    "validate": "bun run typecheck && bun run test",
    "format": "prettier --write .",
    "update-version": "bun run scripts/update-version.ts",
    "db:migrate": "bun --cwd packages/database run migrate",
    "db:studio": "bun run --cwd packages/database studio",
    "drizzle:studio": "bun run --cwd packages/database studio",
    "seed:core-models": "bun --bun packages/database/src/scripts/seed-core-model-presets.ts"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@swc/core": "^1.13.1",
    "@swc/wasm": "^1.13.1",
    "@types/archiver": "^6.0.3",
    "@types/bcrypt": "^6.0.0",
    "@types/bun": "latest",
    "@types/html-to-text": "^9.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/marked": "^6.0.0",
    "@types/path-browserify": "^1.0.3",
    "@types/picomatch": "^3.0.2",
    "@types/sanitize-html": "^2.16.0",
    "@types/turndown": "^5.0.5",
    "archiver": "^7.0.1",
    "bun-types": "latest",
    "drizzle-kit": "^0.31.4",
    "esbuild": "^0.25.8",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@ai-sdk-tools/devtools": "^0.6.1",
    "@ai-sdk/anthropic": "^2.0.27",
    "@ai-sdk/google": "^2.0.20",
    "@ai-sdk/groq": "^2.0.24",
    "@ai-sdk/openai": "^2.0.49",
    "@ai-sdk/openai-compatible": "^1.0.22",
    "@ai-sdk/ui-utils": "^1.2.11",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hey-api/client-fetch": "^0.10.2",
    "@hono/swagger-ui": "^0.5.1",
    "@hono/zod-openapi": "^1.1.0",
    "@hono/zod-validator": "^0.4.3",
    "@modelcontextprotocol/sdk": "^1.13.0",
    "@mozilla/readability": "^0.6.0",
    "@openrouter/ai-sdk-provider": "^1.2.0",
    "@promptliano/shared": "workspace:*",
    "@promptliano/ui": "workspace:*",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/zod-adapter": "^1.129.8",
    "ai": "^5.0.60",
    "bcrypt": "^6.0.0",
    "cheerio": "^1.1.2",
    "date-fns": "^4.1.0",
    "fast-xml-parser": "^5.2.4",
    "gray-matter": "^4.0.3",
    "hono": "^4.7.11",
    "hono-rate-limiter": "^0.4.2",
    "html-to-text": "^9.0.5",
    "ignore": "^7.0.5",
    "isomorphic-dompurify": "^2.26.0",
    "js-yaml": "^4.1.0",
    "jsdom": "^27.0.0",
    "jsonwebtoken": "^9.0.2",
    "linkedom": "^0.18.12",
    "marked": "^16.1.1",
    "openai": "^6.3.0",
    "pdf-parse": "^2.2.3",
    "picomatch": "^4.0.2",
    "robots-parser": "^3.0.1",
    "sanitize-html": "^2.17.0",
    "simple-git": "^3.28.0",
    "tiktoken": "^1.0.22",
    "turndown": "^7.2.1",
    "zod": "^4.1.12",
    "zod-validation-error": "^4.0.1"
  },
  "resolutions": {
    "drizzle-zod": "^0.8.3"
  },
  "private": true
}