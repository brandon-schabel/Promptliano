{
  "name": "promptliano-core",
  "module": "index.ts",
  "version": "0.11.0",
  "type": "module",
  "scripts": {
    "dev": "bun run scripts/start-dev.ts",
    "dev:client": "bun run scripts/start-client-dev.ts",
    "dev:server": "bun run scripts/start-server-dev.ts",
    "mcp:inspector": "bun x @modelcontextprotocol/inspector",
    "build": "bun run scripts/build-all.ts",
    "build:binaries": "bun run scripts/build-binaries.ts",
    "test": "bun run --filter '*' test",
    "typecheck": "bun run --filter '*' typecheck",
    "validate": "bun run typecheck && bun run test",
    "format": "prettier --write .",
    "update-version": "bun run scripts/update-version.ts",
    "db:migrate": "bun run -C packages/database migrate",
    "db:studio": "bun run --cwd packages/database studio",
    "drizzle:studio": "bun run --cwd packages/database studio"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@swc/core": "^1.13.1",
    "@swc/wasm": "^1.13.1",
    "@types/archiver": "^6.0.3",
    "@types/bun": "latest",
    "@types/js-yaml": "^4.0.9",
    "@types/marked": "^6.0.0",
    "@types/path-browserify": "^1.0.3",
    "@types/picomatch": "^3.0.2",
    "archiver": "^7.0.1",
    "bun-types": "latest",
    "drizzle-kit": "^0.31.4",
    "esbuild": "^0.25.8",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/google": "^1.2.19",
    "@ai-sdk/groq": "^1.2.9",
    "@ai-sdk/openai": "^1.3.22",
    "@ai-sdk/openai-compatible": "^0.2.14",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hey-api/client-fetch": "^0.10.2",
    "@hono/swagger-ui": "^0.5.1",
    "@hono/zod-openapi": "^1.1.0",
    "@hono/zod-validator": "^0.4.3",
    "@modelcontextprotocol/sdk": "^1.13.0",
    "@openrouter/ai-sdk-provider": "^0.4.6",
    "@promptliano/api-client": "workspace:*",
    "@promptliano/schemas": "workspace:*",
    "@promptliano/services": "workspace:*",
    "@promptliano/shared": "workspace:*",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/zod-adapter": "^1.129.8",
    "ai": "^4.3.16",
    "fast-xml-parser": "^5.2.4",
    "gray-matter": "^4.0.3",
    "hono": "^4.7.11",
    "hono-rate-limiter": "^0.4.2",
    "ignore": "^7.0.5",
    "isomorphic-dompurify": "^2.26.0",
    "js-yaml": "^4.1.0",
    "marked": "^16.1.1",
    "openai": "^4.104.0",
    "picomatch": "^4.0.2",
    "simple-git": "^3.28.0",
    "zod": "^4.1.5",
    "zod-validation-error": "^4.0.1"
  },
  "resolutions": {
    "zod": "^4.1.5",
    "drizzle-zod": "^0.8.3"
  },
  "private": true
}
