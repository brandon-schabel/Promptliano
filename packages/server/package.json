{
  "name": "octoprompt",
  "module": "server.ts",
  "type": "module",
  "version": "1.0.7",
  "scripts": {
    "test": "bun test src/",
    "test:watch": "bun test --watch src/",
    "migrate": "drizzle-kit generate",
    "push": "drizzle-kit push",
    "start": "bun server.ts",
    "dev": "DEV=true bun --watch server.ts",
    "build": "bun run build.ts",
    "start:prod": "cd dist && ./start.sh",
    "check-process-id": "sudo lsof -i :3000",
    "kill-process-id": "sudo kill -9 $(sudo lsof -i :3000 | awk 'NR==2{print $2}')"
  },
  "devDependencies": {
    "bun-types": "latest",
    "drizzle-kit": "^0.28.1",
    "drizzle-orm": "^0.36.4",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "drizzle-orm": "^0.36.3"
  },
  "dependencies": {
    "@bnk/ai": "^1.0.0",
    "@bnk/router": "1.0.6",
    "@bnk/websocket-manager": "1.0.0",
    "@types/archiver": "^6.0.3",
    "archiver": "^7.0.1",
    "better-sqlite3": "^11.7.2",
    "openai": "^4.78.1",
    "shared": "workspace:*",
    "zod": "^3.24.1"
  }
}
