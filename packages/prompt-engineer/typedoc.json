{
  "$schema": "https://typedoc.org/schema.json",
  "name": "@promptliano/prompt-engineer",
  "entryPoints": ["src/index.ts"],
  "out": "docs/api",
  "plugin": [
    "typedoc-plugin-markdown",
    "typedoc-plugin-missing-exports",
    "typedoc-plugin-mermaid"
  ],
  "readme": "README.md",
  "tsconfig": "./tsconfig.json",
  "excludePrivate": true,
  "excludeInternal": true,
  "excludeExternals": false,
  "includeVersion": true,
  "searchInComments": true,
  "sourceLinkTemplate": "https://github.com/promptliano/promptliano/blob/main/{path}#L{line}",
  "navigationLinks": {
    "GitHub": "https://github.com/promptliano/promptliano",
    "NPM": "https://www.npmjs.com/package/@promptliano/prompt-engineer"
  },
  "categorizeByGroup": true,
  "defaultCategory": "Core",
  "categoryOrder": [
    "Core",
    "Optimizers",
    "Security",
    "Multi-Modal",
    "Benchmarks",
    "Strategies",
    "Analyzers",
    "Plugins",
    "Types",
    "*"
  ],
  "githubPages": false,
  "hideGenerator": false,
  "theme": "default",
  "lightHighlightTheme": "github-light",
  "darkHighlightTheme": "github-dark",
  "skipErrorChecking": true,
  "validation": {
    "notExported": false,
    "invalidLink": true,
    "notDocumented": false
  }
}