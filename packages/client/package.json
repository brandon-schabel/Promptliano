{
  "name": "client",
  "private": true,
  "version": "0.12.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b tsconfig.app.json && vite build",
    "build:prod": "tsc -b tsconfig.app.json && vite build",
    "preview": "vite preview",
    "test": "bun test src/",
    "test:unit": "bun test --pattern='**/*.test.{ts,tsx}' --ignore='**/e2e/**' src/",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:basic": "playwright test --config=playwright-basic.config.ts basic-smoke.spec.ts",
    "test:e2e:basic:ui": "playwright test --config=playwright-basic.config.ts --ui basic-smoke.spec.ts",
    "test:e2e:ci": "playwright test --config=playwright-ci.config.ts",
    "test:e2e:fast": "playwright test --config=playwright-fast.config.ts",
    "test:e2e:visual": "playwright test --config=playwright-visual.config.ts",
    "test:e2e:visual:update": "playwright test --config=playwright-visual.config.ts --update-snapshots",
    "test:codegen": "playwright codegen http://localhost:5173",
    "test:report": "playwright show-report",
    "test:e2e:production": "bun run e2e/scripts/test-execution-coordinator.ts production",
    "test:e2e:development": "bun run e2e/scripts/test-execution-coordinator.ts development",
    "test:e2e:smoke": "bun run e2e/scripts/test-execution-coordinator.ts smoke",
    "test:e2e:suite": "bun run e2e/scripts/test-execution-coordinator.ts production && bun run e2e/scripts/test-execution-coordinator.ts ci",
    "test:e2e:comprehensive": "bun run e2e/scripts/test-execution-coordinator.ts production --headed",
    "typecheck": "tsc -b tsconfig.app.json",
    "check-db-imports": "bun run scripts/check-db-imports.ts",
    "validate-types": "bun run typecheck && bun run check-db-imports"
  },
