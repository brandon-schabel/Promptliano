{
  "name": "@promptliano/storage",
  "version": "0.9.0",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "benchmark": "bun run src/benchmarks/sqlite-performance.ts",
    "test": "NODE_ENV=test bun test",
    "test:debug": "NODE_ENV=test bun test --timeout 30000",
    "typecheck": "tsc --noEmit",
    "docs:generate": "typedoc",
    "docs:markdown": "typedoc --plugin typedoc-plugin-markdown --out docs/api-markdown",
    "docs:watch": "typedoc --watch",
    "docs:serve": "npx serve docs/api",
    "docs:clean": "rm -rf docs/api docs/api-markdown"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
